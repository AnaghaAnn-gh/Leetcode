class Solution:
    def simplifyPath(self, path: str) -> str:
        stack = []
        cur = path.split("/")
        for i in cur :
            if i=="..":
                if stack:
                    stack.pop()
            elif i!="" and i!=".":
                stack.append(i)
        
        res = "/"+"/".join(stack)
        return res
         
            